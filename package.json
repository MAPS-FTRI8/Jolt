{
  "name": "jolt",
  "description": "Chat app",
  "version": "1.0.0",
  "main": "webpack.config.js",
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test-coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "start": "NODE_ENV=production nodemon server/server.js",
    "build": "NODE_ENV=production webpack",
    "dev": "concurrently \"cross-env NODE_ENV=development nodemon server/server.js\" \"cross-env NODE_ENV=development webpack serve --open\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MAPS-Jolt/Jolt.git"
  },
  "author": "Marshall Kim, Anu Sharma, Peter Steinberg, Simon Lee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MAPS-Jolt/Jolt/issues"
  },
  "homepage": "https://github.com/MAPS-Jolt/Jolt#readme",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.2",
    "babel-loader": "^8.2.4",
    "concurrently": "^6.0.2",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "express": "^4.18.1",
    "mongoose": "^6.3.5",
    "nodemon": "^2.0.16",
    "react": "^18.1.0",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.2",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.8.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.17.12",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "babel-jest": "^28.1.1",
    "babel-loader": "^8.2.4",
    "expect-cookies": "^0.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.1",
    "node-sass": "^7.0.1",
    "supertest": "^6.2.3",
    "url-loader": "^4.1.1",
    "webpack-dev-server": "^4.8.1"
  }
}
